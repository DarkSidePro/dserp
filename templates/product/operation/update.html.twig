{% extends "_partials/body.html.twig" %}
{% block stylesheets %}
    {{ parent() }}
    <link rel='stylesheet' href="https://cdn.datatables.net/1.10.23/css/jquery.dataTables.min.css">
    <link rel='stylesheet' href="https://cdn.datatables.net/select/1.3.1/css/select.dataTables.min.css">
    <link rel='stylesheet' href="https://cdn.datatables.net/buttons/1.6.5/css/buttons.dataTables.min.css">
    <link rel='stylesheet' href="https://cdn.datatables.net/select/1.3.1/css/select.dataTables.min.css">
{% endblock %}
{% block title %} - Product operation update{% endblock %}
{% block breadcrumb_link %}
    <li class="breadcrumb-item">
        <a href="{{ path('product') }}" class="breadcrumb-link">Product</a>
    </li>
{% endblock %}
{% block page_header_title %}
    Product {{ product_name }} update
{% endblock %}
{% block content %}
    <div class='col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12'>
        <div class='card'>
            <div class='card-body'>
                <h5 class='card-header'>Product {{product_name}} operation update
                <div class='card-body'>
                    {{ form_start(form) }}
                    {{ form_end(form) }}
                </div>
            </div>
        </div>
    </div>
{% endblock %}